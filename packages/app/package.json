{
  "dependencies": {
    "@internal/dock": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@internal/shared-config": "workspace:*",
    "@types/node": "^22.15.21",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "taze": "^18.7.1",
    "typescript": "^5.8.3",
    "vite": "^5.4.18"
  },
  "name": "app",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "check": "biome check src",
