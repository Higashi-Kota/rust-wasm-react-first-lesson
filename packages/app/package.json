{
  "dependencies": {
    "@internal/wasm-math": "workspace:*",
    "@internal/wasm-text": "workspace:*",
    "@internal/wasm-utils": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@internal/shared-config": "workspace:*",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.21",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "autoprefixer": "^10.4.21",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "taze": "^18.7.1",
    "typescript": "^5.8.3",
    "vite": "^5.4.18",
    "vitest": "^3.1.4"
